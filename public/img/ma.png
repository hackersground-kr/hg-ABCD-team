    function checkDomainInContent(content) {
        var urlPattern = /(https?)?:?(\/\/)?[a-zA-Z0-9-.]+?[a-zA-Z0-9-]+\.[a-zA-Z]+\/?/;

        return urlPattern.test(content);
    }

    function checkProhibitedWords(content) {
        var prohibitedWords = [
            "새끼", "fuck", "씨발", "좆까", "존나", "썅", "미친놈", "미친년", "닥쳐", "꺼져", "병신", "지랄", "염병", 
            "select", "delete", "update", "alter", "drop", "https", "http", "ftp"
        ];

        content = content.toLowerCase();
        for (var i = 0; i < prohibitedWords.length; i++) {
            if (content.indexOf(prohibitedWords[i].toLowerCase()) !== -1) {
                return true;
            }
        }
        
        return false;
    }

    function checkEmoji(content) {
        var emojiPattern = /[\u{1F600}-\u{1F64F}]/gu;

        return emojiPattern.test(content);
    }


